{
  "expo": {
    "name": "Gadgets-Shop",
    "slug": "Gadgets-Shop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/a.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/a.jpg",
        "backgroundColor": "#FF0000" 
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/a.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/aaaaa.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#FF0000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
